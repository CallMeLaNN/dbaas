{
  "name": "dbaas",
  "version": "0.1.0",
  "description": "Database as a service. Expose db to REST and GraphQL.",
  "repository": "https://bitbucket.org/",
  "author": "Mazlan <git@lann.me>",
  "license": "MIT",
  "engines": {
    "node": ">=16.x"
  },
  "type": "commonjs",
  "scripts": {
    "lint": "npm-run-all -n -s 'lint:*'",
    "lint:pretty": "prettier --loglevel=warn --write . '!**/*.{js,ts}'",
    "lint:es": "eslint --ext .js,.ts --fix .",
    "build": "run-s -n lint build-ts",
    "build-ts": "tsc -p src/extensions/tsconfig.json --pretty",
    "clean": "rimraf ./extensions ./src/extensions/tsconfig.tsbuildinfo",
    "rebuild": "run-s -n clean build",
    "start": "nodemon --config nodemon.gulp.json",
    "watch": "run-s start",
    "serve": "gulp serve",
    "upgrade-yarn": "yarn set version latest",
    "upgrade-sdks": "yarn sdks"
  },
  "dependencies": {
    "@directus/shared": "^9.6.0",
    "directus": "^9.6.0",
    "express": "^4.17.1",
    "fancy-log": "^1.3.3",
    "knex": "^0.95.14",
    "node-fetch": "^2",
    "node-graceful": "^3.1.0",
    "pino": "6.13.3",
    "pino-colada": "^2.1.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4",
    "@types/fancy-log": "^1.3.1",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/pino": "^6",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@yarnpkg/sdks": "^2.5.1-rc.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "4.6.3"
  },
  "packageManager": "yarn@3.1.0"
}
